---
import { getTopics } from "../../../lib/zenn";

export const getStaticPaths = async () => {
  const topics = await getTopics();
  return topics.map((topic) => {
    return {
      params: { topic: topic },
    };
  });
};

const { topic } = Astro.params;
return Astro.rewrite(`topics/${topic}/1`);
---
